      }
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "devDependencies": {
    "@types/react": "^16.3.17",
    "babel-plugin-import": "^1.8.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "react-app-rewired": "^1.5.2"
  }
